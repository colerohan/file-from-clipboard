{
  "name": "file-from-clipboard",
  "displayName": "File from Clipboard",
  "description": "Create a new file from clipboard contents via context menu",
  "version": "0.0.1",
  "publisher": "colerohan",
  "engines": {
    "vscode": "^1.89.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:fileFromClipboard.create"
  ],
  "contributes": {
    "commands": [
      {
        "command": "fileFromClipboard.create",
        "title": "Paste as New Fileâ€¦"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "fileFromClipboard.create",
          "group": "navigation@6",
          "when": "explorerResourceIsFolder || resourceScheme == 'file'"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "prepublish": "npm run compile"
  },
